start simulation MinioCSVTest
  # Test CSV export to MinIO
  grid.size = 100 m
  grid.low = 0 degrees latitude, 0 degrees longitude
  grid.high = 0.001 degrees latitude, 0.001 degrees longitude
  steps.low = 0 count
  steps.high = 10 count
  exportFiles.csv = "minio://josh-test-bucket/results/test_{replicate}.csv"

  export.avgAge.end = mean(TestIndividual.age)
  export.avgHeight.end = mean(TestIndividual.height)
end simulation

start patch Default
  TestIndividual.init = create 100 count of TestIndividual
end patch

start organism TestIndividual
  age.init = 0 year
  age.step = prior.age + 1 year

  height.init = 1.0 m
  height.step = prior.height + 0.1 m
end organism

start unit year
  alias years
end unit
