start simulation BackpressureTest
  # Test queue backpressure with large dataset and small queue
  grid.size = 100 m
  grid.low = 0 degrees latitude, 0 degrees longitude
  grid.high = 0.001 degrees latitude, 0.001 degrees longitude
  steps.low = 0 count
  steps.high = 10 count
  exportFiles.patch = "minio://josh-test-bucket/results/backpressure.csv"
end simulation

start patch Default
  TestIndividual.init = create 1000 count of TestIndividual

  export.avgValue.step = mean(TestIndividual.value)
end patch

start organism TestIndividual
  value.init = 0 count
  value.step = prior.value + 1 count
end organism
