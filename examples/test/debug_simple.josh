start simulation SimpleDebug
  grid.size = 1000 m
  grid.low = 34.0 degrees latitude, -116.5 degrees longitude
  grid.high = 34.5 degrees latitude, -116.0 degrees longitude

  steps.low = 0 count
  steps.high = 3 count

  exportFiles.agent = "memory://editor/organisms"
  debugFiles.organism = "file:///tmp/josh_debug_simple.txt"
end simulation

start patch Default
  TestTree.init = create 2 count of TestTree
end patch

start organism TestTree
  age.init = 0 years
  age.step = prior.age + 1 years

  # Simple debug output
  debugMsg.step = debug("Tree age:", age, "geoKey:", geoKey)

  export.age.step = age
  export.geoKey.step = geoKey
end organism

start unit year
  alias years
end unit
