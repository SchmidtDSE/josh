start simulation JobConfigReplicateTest
  grid.size = 1 count
  grid.low = 0 m latitude, 0 m longitude
  grid.high = 100 m latitude, 100 m longitude
  steps.low = 0 count
  steps.high = 3 count
  
  exportFiles.patch = "file:///tmp/job_config_replicate_test.csv"
end simulation

start patch Default
  Tree.init = create 2 count of Tree
end patch

start organism Tree
  height.init = config example.testVar1
  height.step = prior.height + config other.testVar2
  
  age.init = 0 year
  age.step = prior.age + 1 year
  
  # Add some stochastic behavior to ensure different replicate outputs
  randomGrowth.init = 0 meters
  randomGrowth.step = sample uniform from 0 meters to config other.testVar1
end organism

start unit year
  alias years  
  alias yr
  alias yrs
end unit